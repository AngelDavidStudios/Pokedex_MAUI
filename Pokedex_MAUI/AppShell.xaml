<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Pokedex_MAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Pokedex_MAUI.MVVM.Views"
    xmlns:contentViews="clr-namespace:Pokedex_MAUI.MVVM.Views.ContentViews"
    Shell.TabBarIsVisible="False">

    <ShellContent
        Title="Login"
        ContentTemplate="{DataTemplate local:LoginPageView}"
        Route="LoginPageView"
        Shell.FlyoutBehavior="Disabled"
        Shell.TabBarIsVisible="False"/>
    
    <ShellContent
        Title="HomeView"
        ContentTemplate="{DataTemplate local:HomeView}"
        Route="HomeView"
        Shell.FlyoutBehavior="Disabled"
        Shell.TabBarIsVisible="False"/>

</Shell>
